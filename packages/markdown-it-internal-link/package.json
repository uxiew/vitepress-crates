{
  "name": "@ver5/markdown-it-internal-link",
  "type": "module",
  "version": "1.0.0",
  "description": "A markdown-it plugin that link other pages",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/uxiew/vitepress-crates.git",
    "directory": "packages/markdown-it-token"
  },
  "keywords": [
    "vitepress",
    "markdown",
    "markdown-it",
    "markdown-it-plugin",
    "markdown-it-internal-link"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "stub": "unbuild --stub",
    "dev": "unbuild --stub",
    "build": "unbuild",
    "package:publish": "pnpm build && pnpm publish --access public --no-git-checks"
  },
  "peerDependencies": {
    "markdown-it": ">=14.0.0"
  },
  "devDependencies": {
    "@types/markdown-it": "^14.1.1"
  }
}
